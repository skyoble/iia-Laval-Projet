{% extends 'base.html.twig' %}

{% block title %}Liste des utilisateurs{% endblock %}

{% block body %}
<div class="container">
    <h1>Liste des utilisateurs</h1>

    <table class="table table-striped">
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Pr√©nom</th>
                <th>age</th>
                <th>Email</th>
            </tr>
        </thead>

        <tbody>
            {% for seller in sellers %}
                <tr>
                    <td>{{ seller.id }}</td>
                    <td>{{ seller.name }}</td>
                    <td>{{ seller.userName }}</td>
                    <td>{{ seller.age }}</td>
                    <td>{{ seller.email }}</td>
                </tr>
            {% endfor %}
        </tbody>
    </table>
     
</div>
{% endblock %}
